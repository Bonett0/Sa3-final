

<%- include("includes/header") %>


<main class="website_content">

    <!--  -->
    <div>
        <h2>Challenges</h2>
    </div>
    <div class="scroll_div">
        <div class="cover">
            <div class="scroll_images">
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
                <div class="child"><img class="child_img" src="/static/img/myimg.jpg" alt=""></div>
            </div>
        </div>
    </div>


    <!--  -->
    <div class="try">
        <h2>Play</h2>
    </div>
    <section class="playing_modes">
        <img src="/static/img/1vs1.jpg" style="height: 150px; width:250px" alt="">
        <img src="/static/img/1vs1.jpg" style="height: 150px; width:250px; padding-left: 40px;" alt="">
    </section>


    <!-- Score Board -->
    <div class="try">
        <h2>Scoreboard</h2>
    </div>



    <div class="lboard_section">
        <div class="lboard_wrap">
            <% for (let i = 0; i < 10; i++) { %>
                <div class="lboard_mem">
                    <div class="img">
                        <img src="/static/img/avatar.png" alt="picture_1">
                    </div>
                    <div class="name_bar">
                        <p><span>1.</span> Charles John</p>
                        <div class="bar_wrap">
                            <div class="inner_bar" style="width: 95%"></div>
                        </div>
                    </div>
                    <div class="points">
                        195 points
                    </div>
                </div>
            <% } %>
        </div>
    </div>

    



  
</main>





